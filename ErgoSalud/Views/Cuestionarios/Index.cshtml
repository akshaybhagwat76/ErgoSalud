@model IEnumerable<ErgoSalud.Models.ERGOS_Cuestionarios_N01>

@{
    ViewBag.Title = "Index";
}

<h2>Cuestionarios</h2>
<hr />
<p>
    @Html.ActionLink("Agregar Cuestionario", "Create", null, new { @class = "btn btn-success" })
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cuestionario)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Cuestionario)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.id_cuestionario }) |
                    @Html.ActionLink("Details", "Details", new { id = item.id_cuestionario }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.id_cuestionario })
                </td>
            </tr>
        }

    </table>
